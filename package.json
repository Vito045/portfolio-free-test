{
  "name": "Yaron_pariente_portfolio",
  "version": "1.0.0",
  "description": "Yaron pariente portfolio webpage",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass public/sass/main.scss public/css/style.css -w",
    "start": "node ./src/index.js",
    "start2": "live-server",
    "start1": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.comp.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css",
    "start:js": "env-cmd ./config/dev.env node src/index.js",
    "dev:js": "env-cmd ./config/dev.env nodemon src/index.js",
    "development": "npm-run-all --parallel watch:sass dev:js"
  },
  "author": "Jonas",
  "license": "ISC",
  "dependencies": {
    "express-fileupload": "^1.1.5",
    "socketio-file-upload": "^0.7.0",
    "utf8": "^3.0.0",
    "autoprefixer": "^7.1.4",
    "concat": "^1.0.3",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.1",
    "postcss-cli": "^4.1.1",
    "lodash": "^4.17.11",
    "mongodb": "^3.2.2",
    "mongoose": "^5.4.20",
    "multer": "^1.4.1",
    "socket.io": "^2.2.0",
    "socketio": "^1.0.0",
    "validator": "^10.11.0",
    "bcryptjs": "^2.4.3",
    "env-cmd": "^8.0.2",
    "express": "^4.16.4",
    "file-system": "^2.2.2",
    "hbs": "^4.0.3",
    "jsonwebtoken": "^8.5.1",
    "browser-image-compression": "^1.0.6",
    "express-fileupload": "^1.1.5",
    "imagemagick": "^0.1.3",
    "jimp": "^0.6.4",
    "sharp": "^0.23.0",
    "socketio-file-upload": "^0.7.0",
    "utf8": "^3.0.0"
  }
}
